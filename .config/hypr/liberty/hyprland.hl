monitor=eDP-1,2560x1600@165,0x0,1.6

input {
  kb_layout = de
}

xwayland {
  # enabled = false
  force_zero_scaling = true
}

env = GDK_SCALE,2

windowrule = workspace 3 silent,class:vesktop
windowrule = workspace 4 silent,class:steam
windowrule = workspace 5 silent,class:steam_app_.*
windowrule = workspace 5 silent,class:^Minecraft.*$
